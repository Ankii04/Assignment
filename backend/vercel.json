{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "env": {
        "MONGODB_URI": "@mongodb_uri",
        "GEMINI_API_KEY": "@gemini_api_key",
        "GOOGLE_API_KEY": "@google_api_key",
        "GOOGLE_CSE_ID": "@google_cse_id"
    }
}